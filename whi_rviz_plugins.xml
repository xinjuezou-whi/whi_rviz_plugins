<library path="lib/libwhi_rviz_plugins">
  <class name="whi_rviz_plugins/Battery"
         type="whi_rviz_plugins::DisplayBat"
         base_class_type="rviz::Display">
    <description>
      display battery power information
    </description>
    <message_type>whi_interfaces/WhiBattery</message_type>
  </class>
  <class name="whi_rviz_plugins/Navi_waypoints"
         type="whi_rviz_plugins::WaypointsDisplay"
         base_class_type="rviz::Display">
    <description>
      A panel widget to for waypoints of navigation
    </description>
  </class>
  <class name="whi_rviz_plugins/Map_saver"
         type="whi_rviz_plugins::MapSaverPanel"
         base_class_type="rviz::Panel">
    <description>
      A panel widget for save map with map_server
    </description>
  </class>
  <class name="whi_rviz_plugins/Teleop"
         type="whi_rviz_plugins::DisplayTeleop"
         base_class_type="rviz::Display">
    <description>
      Generic keyboard teleop
    </description>
  </class>
  <class name="whi_rviz_plugins/Video_stream"
         type="whi_rviz_plugins::VideoStreamDisplay"
         base_class_type="rviz::Display">
    <description>
      Video stream display
    </description>
  </class>
  <class name="whi_rviz_plugins/Navi_namespace"
         type="whi_rviz_plugins::NaviNsTool"
         base_class_type="rviz::Tool">
    <description>
      2D Navigation initial pose and goal operation with namespace
    </description>
  </class>
  <class name="whi_rviz_plugins/Navi_state"
         type="whi_rviz_plugins::DisplayState"
         base_class_type="rviz::Display">
    <description>
      Navigation status
    </description>
  </class>
  <class name="whi_rviz_plugins/Navi_robot_model_viewer"
         type="whi_rviz_plugins::DisplayRobotModelViewer"
         base_class_type="rviz::Display">
    <description>
      Viewer of robot model
    </description>
  </class>
  <class name="whi_rviz_plugins/2D_line_chart"
         type="whi_rviz_plugins::Display2DLineChart"
         base_class_type="rviz::Display">
    <description>
      2D line chart
    </description>
  </class>
</library>
